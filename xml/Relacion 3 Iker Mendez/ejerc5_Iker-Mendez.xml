<?xml version="1.0" encoding="UTF-8"?>
<árboles>
    <árbol>
        <nombre_científico>Acer monspessulanum</nombre_científico>
        <nombre_común>Arce de Montpellier, Arce menor</nombre_común>
        <vegetación>Caducifolio</vegetación>
        <altura>De 6 a 10 metros</altura>
        <forma_estructura>Copa esférica, Tronco principal recto con bifurcaciones. Ramaje colgante</forma_estructura>
        <color_primavera> Haz verde brillante, envés verde blanquecino</color_primavera>
        <resistencia_heladas>Heladas fuertes (hasta -15ºC)</resistencia_heladas>
    </árbol>
    <árbol>
        <nombre_científico>Olea europea</nombre_científico>
        <nombre_común>Olivo</nombre_común>
        <vegetación>Perenne</vegetación>
        <altura>De 8 a 15 metros</altura>
        <forma_estructura>Copa irregular. Tronco principal irregular con bifurcaciones</forma_estructura>
    </árbol>
    <árbol>
        <nombre_científico>Ramaje tortuoso</nombre_científico>
        <color_primavera>Haz verde oscuro, envés verde plateado</color_primavera>
        <resistencia_heladas>Heladas medias (hasta -10ºC)</resistencia_heladas>
    </árbol>
    <árbol>
        <nombre_científico>Platanus orientalis</nombre_científico>
        <nombre_común>Platano</nombre_común>
        <vegetación>Caducifolio</vegetación>
        <altura>De 20 a 25 metros</altura>
        <forma_estructura>Copa ovoidal. Tronco principal recto. Ramaje expandido</forma_estructura>
        <color_primavera> Haz verde medio, enves verde claro</color_primavera>
        <color_otoño>Ocre</color_otoño>
        <resistencia_heladas>Heladas fuertes (hasta -20ºC)</resistencia_heladas>
    </árbol>
    <árbol>
        <nombre_científico>Quercus ilex</nombre_científico>
        <nombre_común>Encina</nombre_común>
        <vegetación>Perenne</vegetación>
        <altura>En torno a 25 metros</altura>
        <forma_estructura>Copa esférica o elíptica irregular. Tronco principal recto. Ramaje tortuoso</forma_estructura>
        <color_primavera>Plateado en hojas jóvenes. En hojas antiguas, haz verde oscuro,
        envés plateado</color_primavera>
        <resistencia_heladas>Heladas fuertes (hasta -15ºC)</resistencia_heladas>
    </árbol>
</árboles>